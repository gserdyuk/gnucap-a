 *------------------------------------------------------------------
 * i=ar*(v-v0)^3 + b model.
 * netlist syntax:
 * device:  Axxxx n+ n- mname Ar=<area>  v0 = <v0> b=<b>
 * model:   .model mname A
 * properties - singularity (di/dv=0) at v=v0
 */
h_headers {
  enum region_t {REVERSE=-1, UNKNOWN=0, FORWARD=1};
  enum polarity_t {pP = -1, pN = 1};
}
cc_headers {
#include "e_aux.h"
  static bool dummy=false;

}
/*--------------------------------------------------------------------------*/
// GS - here ---------------------------
device BUILT_IN_CUBIC {
  parse_name square;
  model_type BUILT_IN_CUBIC;
  id_letter A;
  circuit {
    ports {a c};
    admittance Yj {a c} eval=Yj;
    }
  tr_probe {
    "v{d}" = "@n_a[V] - @n_c[V]";
    "i{d}" = "@Yj[I] ";
    ij = "@Yj[I]";
  }
  
  device {
    calculated_parameters {
    }
  }
  
  common {
    unnamed area;
    raw_parameters {
      double ar "ar factor"
	name=AR default=1.0 positive;
      double b "b term"
        name=B default=-0.001 positive;
      double v0 "v0 shift"
        name=V0 default=1.0 positive;
        
    }
    calculated_parameters {
    }
  }
  
  
  eval Yj {   // ar*(v-v0)^2 + b
    FPOLY1& y = d->_y[0];
    double volts = y.x;
    double amps  = y.f0;
    trace2(d->long_label().c_str(), volts, amps);

    double polarity= (volts-c->v0)>=0?1:-1;
    
	y.f1 = c->ar *3   *(volts-c->v0)*(volts-c->v0);
	y.f0 = c->ar      *(volts-c->v0)*(volts-c->v0)*(volts-c->v0) + c->b;

  }
}
/*--------------------------------------------------------------------------*/
/*--------------------------------------------------------------------------*/
model BUILT_IN_CUBIC {
  dev_type BUILT_IN_CUBIC;
  hide_base;
  inherit CARD;
  public_keys {
     A dummy=true; //?
  }
  independent {
  }
}

 *------------------------------------------------------------------
 * data structures and defaults for mos model.
 * internal units all mks (meters)
 * but some user input parameters are in cm.
 *
 * netlist syntax:
 * device:  mxxxx d g s b mname <device args> <model card args>
 * model:   .model mname NMOS <args>
 *	or  .model mname PMOS <args>
 */
h_headers {
}
cc_headers {
}
/*--------------------------------------------------------------------------*/
device BUILT_IN_ROZEN {
  parse_name rozen;
  model_type BUILT_IN_ROZEN;
  id_letter A;
  circuit {
    ports {a b c};


    resistor R1 {a c} value=-1;
    

    diode Ddb {b id} ;

    diode Dsb {b is} ;
    

    cpoly_g Ids {id is  g is  id g
		b is  id b} state=idsxxx;
  }
  tr_probe {
    v1   = "@n_a[V] - @n_c[V]";
    v2   = "@n_b[V] - @n_c[V]";
    va   = "@n_a[V] - @n_c[V]";
    vb   = "@n_b[V] - @n_c[V]";
  }
  device {
    calculated_parameters {
    }
  }
  common {
    raw_parameters {
    }
  }
  /*--------------------------------------------------------------------*/
}
/*--------------------------------------------------------------------------*/
/*--------------------------------------------------------------------------*/
model BUILT_IN_ROZEN {
  dev_type BUILT_IN_ROZEN;
  hide_base;
  inherit CARD;
  public_keys {
     A dummy=true; //?
  }
  independent {
  }
}
/*--------------------------------------------------------------------------*/



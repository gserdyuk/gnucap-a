#define DEXP(A,B, C) {                                                        \
        if (A > EXP_THRESHOLD) {                                              \
            B = MAX_EXP;                                                      \
            C = 0;                                                            \
        } else if (A < -30)  {                                                \
            B = MIN_EXP;                                                      \
            C = 0;                                                            \
        } else   {                                                            \
            B = exp(A);                                                       \
            C = B;                                                            \
        }                                                                     \
    }
